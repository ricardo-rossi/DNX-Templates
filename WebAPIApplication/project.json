{
    "webroot": "wwwroot",
    "version": "1.0.0-*",

    "dependencies": {
        "System.Console": "4.0.0-beta-*",
        "Microsoft.AspNet.Mvc": "6.0.0-beta4",
        "Microsoft.AspNet.Server.IIS": "1.0.0-beta4",
        "Microsoft.AspNet.Server.WebListener": "1.0.0-beta4",
        "Microsoft.AspNet.StaticFiles": "1.0.0-beta4",
        "Kestrel": "1.0.0-beta4"
    },

    "commands": {
        "web": "Microsoft.AspNet.Hosting --server Kestrel --server.urls http://localhost:5001"
    },

    "frameworks": {
        "dnx451": {},
        "dnxcore50": {}
    },

    "exclude": [
    "wwwroot",
    "node_modules",
    "bower_components"
  ],
    "publishExclude": [
    "node_modules",
    "bower_components",
    "**.xproj",
    "**.user",
    "**.vspscc"
  ]
}
